{
  "expo": {
    "name": "Latacunga Clean",
    "slug": "latacunga-clean",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E7D32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.latacunga.clean",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Latacunga Clean necesita tu ubicación para mostrarte puntos de reciclaje cercanos.",
        "NSLocationAlwaysUsageDescription": "Latacunga Clean necesita tu ubicación para registrar reportes.",
        "NSCameraUsageDescription": "Necesitamos acceso a tu cámara para tomar fotos de reportes.",
        "NSPhotoLibraryUsageDescription": "Necesitamos acceso a tus fotos para adjuntar imágenes a reportes."
      },
      "config": {
        "googleMapsApiKey": "pk.eyJ1IjoiYmpzYW5nb2x1aXNhIiwiYSI6ImNtZ2R5ZnJrcjE3Y3oyaXBrbTd3enpoYW4ifQ.lLhsDku7duNx3lCexMcgbQ"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#2E7D32"
      },
      "package": "com.latacunga.clean",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "pk.eyJ1IjoiYmpzYW5nb2x1aXNhIiwiYSI6ImNtZ2R5ZnJrcjE3Y3oyaXBrbTd3enpoYW4ifQ.lLhsDku7duNx3lCexMcgbQ"
        }
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Latacunga Clean necesita tu ubicación para mostrarte puntos de reciclaje cercanos."
        }
      ]
    ]
  }
}